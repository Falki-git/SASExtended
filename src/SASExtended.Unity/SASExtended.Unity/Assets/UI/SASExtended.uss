    border-top-width: 1px;
    border-bottom-width: 1px;
}

.side-toggle__container--hover {
    border-left-color: var(--background-lightgray-4-translucent);
    border-right-color: var(--background-lightgray-4-translucent);
    border-top-color: var(--background-lightgray-4-translucent);
    border-bottom-color: var(--background-lightgray-4-translucent);
    border-left-width: 1px;
    border-right-width: 1px;
    border-top-width: 1px;
    border-bottom-width: 1px;
}

.side-toggle__container--active {
    border-left-color: var(--border-active-green);
    border-right-color: var(--border-active-green);
    border-top-color: var(--border-active-green);
    border-bottom-color: var(--border-active-green);
    border-left-width: 1px;
    border-right-width: 1px;
    border-top-width: 1px;
    border-bottom-width: 1px;
}

.side-toggle__led {
    width: 3px;
    height: 20px;
    margin-left: 3px;
    border-top-left-radius: 1px;
    border-bottom-left-radius: 1px;
    border-top-right-radius: 1px;
    border-bottom-right-radius: 1px;
}

.side-toggle--big__led {
    width: 3px;
    height: 20px;
    margin-left: 3px;
    border-top-left-radius: 1px;
    border-bottom-left-radius: 1px;
    border-top-right-radius: 1px;
    border-bottom-right-radius: 1px;
}

.side-toggle--small__led {
    width: 2px;
    height: 8px;
    margin-left: 2px;
    border-top-left-radius: 1px;
    border-bottom-left-radius: 1px;
    border-top-right-radius: 1px;
    border-bottom-right-radius: 1px;
}

.side-toggle__led--checked {
    background-color: var(--led-checked-green);
}

.side-toggle__led--unchecked {
    background-color: var(--led-unchecked-gray);
}

.side-toggle__text {
    flex-grow: 1;
    padding-right: 0;
    -unity-text-align: middle-center;
    padding-top: 0;
    padding-bottom: 0;
    padding-left: 3px;
    -unity-paragraph-spacing: -30px;
}

.side-toggle--big__text {
    flex-grow: 1;
    padding-right: 0;
    -unity-text-align: middle-center;
    padding-top: 0;
    padding-bottom: 0;
    padding-left: 3px;
    -unity-paragraph-spacing: -40px;
    word-spacing: -20px;
    letter-spacing: -10px;
}

.side-toggle--small__text {
    flex-grow: 1;
    padding-right: 0;
    padding-top: 0;
    padding-bottom: 0;
    padding-left: 0;
    -unity-paragraph-spacing: -40px;
    word-spacing: -20px;
    letter-spacing: -10px;
    font-size: 12px;
    -unity-text-align: middle-center;
    left: 3px;
}

.side-toggle__led--disabled {
    background-color: var(--led-disabled-gray);
}

.side-toggle__text--disabled {
    color: var(--background-lightgray-4-translucent);
}

.notification {
    position: absolute;
    bottom: 5px;
    background-color: var(--border-active-green-translucent);
    color: var(--black-1);
    font-size: 12px;
    padding-left: 5px;
    padding-right: 5px;
    padding-top: 5px;
    padding-bottom: 5px;
    -unity-text-align: upper-center;
    opacity: 0;
    transition-duration: 1s;
    left: 5px;
    -unity-font-definition: var(--font-bold);
}

.notification--show {
    transition-duration: 1s;
    opacity: 1;
}

.resize-handle__container {
    flex-grow: 0;
    width: 32px;
    height: 32px;
    position: absolute;
    right: -10px;
    bottom: -10px;
    display: none;
}

.resize-handle {
    flex-grow: 1;
    background-image: url('project://database/Assets/Images/Icons/ELE-Panel-Handle.png?fileID=21300000&guid=59a0e8b5ecc8a374f891341b8927a814&type=3#ELE-Panel-Handle');
    max-height: 8px;
    max-width: 8px;
    left: 10px;
    top: 10px;
    -unity-background-image-tint-color: var(--background-lightgray-4);
}

.attitude__button {
    font-size: 12px;
    height: 80%;
    border-left-width: 1px;
    border-right-width: 1px;
    border-top-width: 1px;
    border-bottom-width: 1px;
    border-top-left-radius: 5px;
    border-bottom-left-radius: 5px;
    border-top-right-radius: 5px;
    border-bottom-right-radius: 5px;
