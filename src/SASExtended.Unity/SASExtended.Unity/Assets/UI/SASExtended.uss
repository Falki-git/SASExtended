    padding-bottom: 0;
    margin-left: 1px;
    -unity-paragraph-spacing: -40px;
    word-spacing: -20px;
    letter-spacing: -10px;
}

.tab-toggle {
    width: 45px;
    flex-direction: row;
    background-color: rgba(0, 0, 0, 0);
    align-items: center;
    justify-content: space-around;
    padding-left: 0;
    padding-right: 0;
    padding-top: 0;
    font-size: 12px;
    height: 25px;
    margin-top: 0;
    margin-bottom: 0;
    border-bottom-left-radius: 0;
    border-bottom-right-radius: 0;
    padding-bottom: 0;
    margin-left: 0;
    margin-right: 0;
    top: 1px;
    border-left-width: 0;
    border-right-width: 0;
    border-top-width: 0;
    border-bottom-width: 0;
}

.tab-toggle__container {
